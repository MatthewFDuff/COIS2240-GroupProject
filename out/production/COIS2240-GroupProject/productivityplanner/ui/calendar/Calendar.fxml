            <HBox fx:id="hboxMonthTitle" alignment="CENTER" maxHeight="100.0" maxWidth="300.0" minHeight="60.0" minWidth="300.0" prefHeight="60.0" prefWidth="300.0" spacing="20.0" style="-fx-background-color: #292929;" HBox.hgrow="NEVER">
